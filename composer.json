{
  "name": "burdadirect/symfony-bundle-bootstrap-forms",
  "description": "HBM Bootstrap Form Bundle - Twig template and form extensions.",
  "authors" : [{
    "name" : "Christian Puchinger",
    "email" : "christian.puchinger@burda.com"
  }],
  "repositories" : [
    {
      "type": "vcs",
      "url": "git@github.com:burdadirect/codestyle.git"
    }
  ],
  "license": "MIT",
  "type": "symfony-bundle",
  "autoload": {
    "psr-4": {
      "HBM\\BootstrapFormBundle\\": "src/"
    }
  },
  "require": {
    "php" : "^8.2.0",
    "symfony/config": "^6.3",
    "symfony/dependency-injection": "^6.3",
    "symfony/form": "^6.3",
    "symfony/http-kernel": "^6.3",
    "symfony/twig-bundle": "^6.3"
  },
  "require-dev": {
    "burdadirect/codestyle": "dev-main",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
